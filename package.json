{
  "name": "projection",
  "productName": "projection",
  "version": "0.0.11-alpha",
  "description": "Projection Engine",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "facobackup",
    "email": "gustavomicaelbarbosa@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "--asar": false
      },
      "platforms": [
        "darwin",
        "linux",
        "win32"
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "projection"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "devContentSecurityPolicy": "default-src * 'self' data: blob: file: 'unsafe-inline' 'unsafe-eval'; script-src * 'self' data: blob: file: 'unsafe-inline' 'unsafe-eval'; connect-src * 'self' data: blob: file: 'unsafe-inline' 'unsafe-eval'; img-src * 'self' data: blob: file: data: blob: 'unsafe-inline'; frame-src * 'self' data: blob: file:; style-src * 'self' data: blob: file: 'unsafe-inline';",
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/entry/window.html",
                  "js": "./public/entry/home.js",
                  "name": "main_window"
                },
                {
                  "html": "./public/entry/window.html",
                  "js": "./public/entry/project.js",
                  "name": "project_window"
                },
                {
                  "html": "./public/entry/window.html",
                  "js": "./public/entry/settings.js",
                  "name": "settings"
                },
                {
                  "html": "./public/entry/window.html",
                  "js": "./public/entry/file.js",
                  "name": "file"
                },
                {
                  "html": "./public/entry/window.html",
                  "js": "./public/entry/help.js",
                  "name": "help"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-async-to-generator": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.17.9",
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.63",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "css-loader": "^6.7.1",
    "electron": "17.4.1",
    "eslint": "^8.16.0",
    "eslint-plugin-react": "^7.30.0",
    "node-loader": "^2.0.0",
    "react-scripts": "^5.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.5",
    "stylelint-config-standard": "^25.0.0",
    "url-loader": "^4.1.1"
  },
  "dependencies": {
    "@f-ui/core": "^0.2.25-alpha",
    "atob": "^2.1.2",
    "babel-runtime": "^6.26.0",
    "buffer": "^6.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "gl-matrix": "^3.4.3",
    "hdrpng": "https://github.com/enkimute/hdrpng.js.git",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-colorful": "^5.5.1",
    "react-dom": "^18.2.0",
    "uuid": "^8.3.2"
  }
}

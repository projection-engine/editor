{
  "name": "electron-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@electron/remote": "2.0.4",
    "@f-ui/core": "0.1.41-alpha",
    "@fontsource/roboto": "4.5.3",
    "assimpjs": "^0.0.7",
    "dexie": "^3.2.0",
    "draco3d": "^1.5.2",
    "electron-is-dev": "^2.0.0",
    "gl-matrix": "^3.4.3",
    "hdrpng": "https://github.com/enkimute/hdrpng.js.git",
    "path-browserify": "^1.0.1",
    "pj-math": "^0.0.6-alpha",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.2",
    "react": "17.0.2",
    "react-colorful": "^5.5.1",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "seed-random": "^2.2.0",
    "systeminformation": "^5.11.3",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4",
    "webpack": "^5.68.0"
  },
  "main": "public/main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "s": "concurrently -k  \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron ."
  },
  "build": {
    "extends": null,
    "appId": "projection-engine",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "wait-on": "^6.0.1"
  }
}
